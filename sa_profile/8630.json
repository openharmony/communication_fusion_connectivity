{
    "process": "partner_device_agent",
    "systemability": [
        {
            "name": 8630,
            "libpath": "/system/lib64/libpartner_device_agent_server.z.so",
            "run-on-create": false,
            "distributed": false,
            "auto-restart":true,
            "dump-level": 1,
            "min_hdi_proxy_version": [],
            "start-on-demand": {
                "allow-update":false,
                "commonevent":[
                    {
                        "name":"usual.event.bluetooth.host.STATE_UPDATE",
                        "value":"1",
                        "conditions": [
                            {
                                "name":"persist.fusion_connectivity.enable_partner_agent",
                                "value":"1"
                            }
                        ]
                    }
                ]
            },
            "stop-on-demand": {
                "param":[
                    {
                        "name":"persist.fusion_connectivity.enable_partner_agent",
                        "value":"0"
                    }
                ]
            }
        }
    ]
}